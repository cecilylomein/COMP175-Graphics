<scenefile>
    <globaldata>
        <diffusecoeff v="0.2"/>
        <specularcoeff v="0.2"/>
        <ambientcoeff v="0.2"/>
    </globaldata>

    <cameradata>
        <pos x="2" y="2" z="2"/>
        <focus x="0" y="0" z="0"/>
        <up x="0" y="1" z="0"/>
        <heightangle v="45"/>
    </cameradata>

    <lightdata>
        <id v="0"/>
        <color r="1" g="1" b="1"/>
        <position x="4" y="4" z="4"/>
    </lightdata>

    <lightdata>
        <id v="1"/>
        <color r="1" g="1" b="1"/>
        <position x="-3" y="3" z="3"/>
    </lightdata>

    <lightdata>
        <id v="2"/>
        <color r="1" g="1" b="1"/>
        <position x="-3" y="3" z="-3"/>
    </lightdata>

    <lightdata>
        <id v="3"/>
        <color r="1" g="1" b="1"/>
        <position x="3" y="3" z="-3"/>
    </lightdata>

    <!-- chicken -->
    <object type="tree" name="eye">
        <transblock>
            <!-- eyeball -->
            <scale x="0.15" y="0.15" z="0.15"/>
            <object type="primitive" name="sphere" >
                <diffuse r="1" g="1" b="1"/>
            </object>
        </transblock>
        <transblock>
            <translate x="0" y="0" z="0.03" />
            <object type="tree">
                <transblock>
                    <!-- iris -->
                    <scale x="0.12" y="0.12" z="0.12"/>
                    <object type="primitive" name="sphere" >
                        <diffuse r="0" g="0.05" b="0.05"/>
                    </object>
                </transblock>
                <transblock>
                    <!-- pupil -->
                    <translate x="0" y="0" z="0.015" />
                    <scale x="0.1" y="0.1" z="0.1"/>
                    <object type="primitive" name="sphere" >
                        <diffuse r="0" g="0" b="0"/>
                    </object>
                </transblock>
            </object>
        </transblock>
    </object>

    <object type="tree" name="wing">
        <transblock>
            <rotate x="0" y="0" z="1" angle="15" />
            <scale x="0.75" y="0.35" z="0.25"/>
            <object type="primitive" name="sphere" >
                <diffuse r="0" g="1" b="1"/>
            </object>
        </transblock>
    </object>

    <!--             -->
    <!-- CHICK BODY  -->
    <!--             -->
    <object type="tree" name="body">
        <transblock>
            <scale x="1.25" y="1" z="1"/>
            <object type="primitive" name="sphere" >
                <diffuse r="0" g="1" b="1"/>
            </object>
        </transblock>
        <transblock>
            <translate x="-0.4" y="0.1" z="0" />
            <rotate x="0" y="0" z="1" angle="45"/>
            <rotate x="0" y="0" z="1" angle="90"/>
            <scale x="1.25" y="0.75" z="1"/>
            <object type="primitive" name="sphere" >
                <diffuse r="0" g="1" b="1"/>
            </object>
        </transblock>
        <transblock>
            <translate x="0" y="0" z="-0.45"/>
            <rotate x="0" y="1" z="0" angle="180" />
            <object type="master" name="wing" >
            </object>
        </transblock>
        <transblock>
            <translate x="0" y="0" z="0.45"/>
            <object type="master" name="wing" >
            </object>
        </transblock>
    </object>

    <object type="tree" name="toe">
        <transblock>
            <translate x="0" y="0" z="0.15" />
            <rotate x="1" y="0" z="0" angle="90"/>
            <scale x="0.1" y="0.35" z="0.1" />
            <object type="primitive" name="cone" >
                <diffuse r="0.95" g="0.35" b="0.1"/>
            </object>
        </transblock>
    </object>

    <!--            -->
    <!-- CHICK LEG  -->
    <!--            -->
    <object type="tree" name="leg">
        <transblock>
            <scale x="0.075" y="0.3" z="0.075" />
            <translate x="0" y="0.5" z="0" />
            <object type="primitive" name="cylinder" >
                <diffuse r="0.95" g="0.35" b="0.1"/>
            </object>
        </transblock>
        <transblock>
            <!-- foot -->
            <object type="tree" >
                <transblock>
                    <rotate x="0" y="1" z="0" angle="120"/>
                    <object type="master" name="toe" >
                    </object>
                </transblock>
                <transblock>
                    <rotate x="0" y="1" z="0" angle="240"/>
                    <object type="master" name="toe" >
                    </object>
                </transblock>
                <transblock>
                    <object type="master" name="toe" >
                    </object>
                </transblock>
            </object>
        </transblock>
    </object>

    <!--            -->
    <!-- CHICK HEAD -->
    <!--            -->
    <object type="tree" name="head">
        <transblock>
            <!-- head -->
            <object type="primitive" name="sphere" >
                <diffuse r="0" g="1" b="1"/>
            </object>
        </transblock>
        <transblock>
            <!-- neck -->
            <translate x="0" y="-0.5" z="-0.1" />
            <rotate x="1" y="0" z="0" angle="15" />
            <scale x="0.5" y="0.5" z="0.5" />
            <object type="primitive" name="cylinder" >
                <diffuse r="1" g="1" b="1"/>
            </object>
        </transblock>
        <transblock>
            <scale x="0.8" y="0.8" z="0.8" />
            <translate x="0" y="0.5" z="0" />
            <object type="tree" >
                <transblock>
                    <!-- head decoration back -->
                    <translate x="0" y="0" z="-0.15" />
                    <rotate x="1" y="0" z="0" angle="-25" />
                    <scale x="0.3" y="0.7" z="0.3" />
                    <object type="primitive" name="sphere" >
                        <diffuse r="1" g="0" b="0"/>
                    </object>
                </transblock>
                <transblock>
                    <!-- head decoration mid -->
                    <translate x="0" y="0" z="0" />
                    <scale x="0.3" y="0.7" z="0.3" />
                    <object type="primitive" name="sphere" >
                        <diffuse r="1" g="0" b="0"/>
                    </object>
                </transblock>
                <transblock>
                    <!-- head decoration front -->
                    <translate x="0" y="0" z="0.15" />
                    <rotate x="1" y="0" z="0" angle="25" />
                    <scale x="0.3" y="0.7" z="0.3" />
                    <object type="primitive" name="sphere" >
                        <diffuse r="1" g="0" b="0"/>
                    </object>
                </transblock>
            </object>
        </transblock>
        <transblock>
            <!-- beak -->
            <translate x="0" y="0" z="0.5" />
            <rotate x="1" y="0" z="0" angle="90"/>
            <scale x="0.5" y="0.5" z="0.5"/>
            <object type="primitive" name="cone" >
                <diffuse r="1" g="1" b="0"/>
            </object>
        </transblock>
        <transblock>
            <!-- eyeballs -->
            <translate x="0" y="0.28" z="0.43" />
            <object type="tree">
                <transblock>
                    <!-- left eye -->
                    <translate x="-0.1" y="0" z="0" />
                    <object type="master" name="eye" >
                        <diffuse r="0.05" g="0.05" b="0.05"/>
                    </object>
                </transblock>
                <transblock>
                    <!-- right eye -->
                    <translate x="0.1" y="0" z="0" />
                    <object type="master" name="eye" >
                        <diffuse r="0.05" g="0.05" b="0.05"/>
                    </object>
                </transblock>
            </object>
        </transblock>
    </object>

    <!--            -->
    <!--   CHICKEN  -->
    <!--            -->
    <object type="tree" name="chicken">
        <transblock>
            <translate x="0" y="0.3" z="0" />
            <scale x="0.5" y="0.5" z="0.5" />
            <object type="tree">
                <transblock>
                    <object type="master" name="body" >
                    </object>
                </transblock>
                <transblock>
                    <translate x="0.3" y="0.85" z="0" />
                    <rotate x="0" y="1" z="0" angle="90" />
                    <scale x="0.65" y="0.65" z="0.65" />
                    <object type="master" name="head" >
                    </object>
                </transblock>
            </object>
        </transblock>
        <transblock>
            <translate x="0" y="0" z="-0.1" />
            <rotate x="0" y="1" z="0" angle="-20" />
            <scale x="0.5" y="0.5" z="0.5" />
            <object type="master" name="leg" >
            </object>
        </transblock>
        <transblock>
            <translate x="0" y="0" z="0.1" />
            <rotate x="0" y="1" z="0" angle="-20" />
            <scale x="0.5" y="0.5" z="0.5" />
            <object type="master" name="leg" >
            </object>
        </transblock>
    </object>

    <!-- horse -->
    <object type="tree" name="horse">
        <transblock>
            <object type="tree" name="horsebody">
                <!-- mane -->
                <transblock>
                    <translate x="0" y=".6" z=".68"/>
                    <rotate x="1" y="0" z="0" angle="45"/>
                    <scale x=".1" y=".9" z=".1"/>
                    <object type="primitive" name="sphere">
                        <diffuse r="1" g="1" b="1"/>
                    </object>
                </transblock>   
                <transblock>
                    <translate x="0.07" y=".55" z=".68"/>
                    <rotate x="1" y="0" z="0" angle="45"/>
                    <rotate x="0" y="1" z="0" angle="-40"/>
                    <scale x=".15" y=".8" z=".03"/>
                    <object type="primitive" name="cube">
                        <diffuse r="1" g="1" b="1"/>
                    </object>
                </transblock> 
                <!-- front torso -->
                <transblock>
                    <translate x="0" y="0" z=".5"/>
                    <scale x=".65" y=".65" z=".65"/>
                    <object type="primitive" name="sphere">
                        <diffuse r=".54" g=".27" b=".07"/>
                    </object>
                </transblock>
                <!-- butt -->
                <transblock>
                    <translate x="0" y="0" z="-.5"/>
                    <scale x=".65" y=".7" z=".65"/>
                    <object type="primitive" name="sphere">
                        <diffuse r=".54" g=".27" b=".07"/>
                    </object>
                </transblock>
                <!-- torso -->
                <transblock>
                    <rotate x="1" y="0" z="0" angle="90"/>
                    <scale x=".58" y="1" z=".58"/>
                    <object type="primitive" name="cylinder">
                        <diffuse r=".54" g=".27" b=".07"/>
                    </object>
                </transblock>
                <!-- legs -->
                <transblock>
                    <object type="tree" name="legs">
                        <!-- Front left -->
                        <transblock>
                            <translate x=".1" y="-.6" z=".5"/>
                            <rotate x="0" y="0" z="1" angle="180"/>
                            <scale x=".2" y=".7" z=".3"/>
                            <object type="primitive" name="cone">
                                <diffuse r=".54" g=".27" b=".07"/>
                            </object>
                        </transblock>
                        <transblock>
                            <translate x=".1" y="-.6" z=".5"/>
                            <object type="tree" name="knees">
                                <transblock>
                                    <translate x="0" y="-.2" z="0"/>
                                    <scale x=".1" y=".2" z=".1"/>
                                    <object type="primitive" name="sphere">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                                <transblock>
                                    <translate x="0" y="-.3" z="0"/>
                                    <!-- <rotate x="1" y="0" z="0" angle="20"/> -->
                                    <scale x=".08" y=".7" z=".08"/>
                                    <object type="primitive" name="cylinder">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                                <transblock>
                                    <translate x="0" y="-.65" z="0"/>
                                    <scale x=".15" y=".15" z=".15"/>
                                    <object type="primitive" name="cone">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                            </object>
                        </transblock>

                        <!-- Front right -->
                        <transblock>
                            <translate x="-.1" y="-.6" z=".5"/>
                            <rotate x="0" y="0" z="1" angle="180"/>
                            <scale x=".2" y=".7" z=".3"/>
                            <object type="primitive" name="cone">
                                <diffuse r=".54" g=".27" b=".07"/>
                            </object>
                        </transblock>
                        <transblock>
                            <translate x="-.1" y="-.6" z=".5"/>
                            <object type="tree" name="knees">
                                <transblock>
                                    <translate x="0" y="-.2" z="0"/>
                                    <scale x=".1" y=".2" z=".1"/>
                                    <object type="primitive" name="sphere">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                                <transblock>
                                    <translate x="0" y="-.3" z="0"/>
                                    <!-- <rotate x="1" y="0" z="0" angle="20"/> -->
                                    <scale x=".08" y=".7" z=".08"/>
                                    <object type="primitive" name="cylinder">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                                <transblock>
                                    <translate x="0" y="-.65" z="0"/>
                                    <scale x=".15" y=".15" z=".15"/>
                                    <object type="primitive" name="cone">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                            </object>
                        </transblock>

                        <!-- Back left -->
                        <transblock>
                            <translate x="-.1" y="-.6" z="-.5"/>
                            <rotate x="0" y="0" z="1" angle="180"/>
                            <scale x=".2" y=".7" z=".3"/>
                            <object type="primitive" name="cone">
                                <diffuse r=".54" g=".27" b=".07"/>
                            </object>
                        </transblock>
                        <transblock>
                            <translate x="-.1" y="-.6" z="-.5"/>
                            <object type="tree" name="knees">
                                <transblock>
                                    <translate x="0" y="-.2" z="0"/>
                                    <scale x=".1" y=".2" z=".1"/>
                                    <object type="primitive" name="sphere">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                                <transblock>
                                    <translate x="0" y="-.3" z="0"/>
                                    <!-- <rotate x="1" y="0" z="0" angle="20"/> -->
                                    <scale x=".08" y=".7" z=".08"/>
                                    <object type="primitive" name="cylinder">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                                <transblock>
                                    <translate x="0" y="-.65" z="0"/>
                                    <scale x=".15" y=".15" z=".15"/>
                                    <object type="primitive" name="cone">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                            </object>
                        </transblock>

                        <!-- Back left -->
                        <transblock>
                            <translate x=".1" y="-.6" z="-.5"/>
                            <rotate x="0" y="0" z="1" angle="180"/>
                            <scale x=".2" y=".7" z=".3"/>
                            <object type="primitive" name="cone">
                                <diffuse r=".54" g=".27" b=".07"/>
                            </object>
                        </transblock>
                        <transblock>
                            <translate x=".1" y="-.6" z="-.5"/>
                            <object type="tree" name="knees">
                                <transblock>
                                    <translate x="0" y="-.2" z="0"/>
                                    <scale x=".1" y=".2" z=".1"/>
                                    <object type="primitive" name="sphere">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                                <transblock>
                                    <translate x="0" y="-.3" z="0"/>
                                    <!-- <rotate x="1" y="0" z="0" angle="20"/> -->
                                    <scale x=".08" y=".7" z=".08"/>
                                    <object type="primitive" name="cylinder">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                                <transblock>
                                    <translate x="0" y="-.65" z="0"/>
                                    <scale x=".15" y=".15" z=".15"/>
                                    <object type="primitive" name="cone">
                                        <diffuse r=".54" g=".27" b=".07"/>
                                    </object>
                                </transblock>
                            </object>
                        </transblock>


                    </object>
                </transblock>
                <!-- neck -->
                <transblock>
                    <translate x="0" y=".5" z=".75"/>
                    <rotate x="1" y="0" z="0" angle="25"/>
                    <scale x=".4" y=".7" z=".5"/>
                    <object type="primitive" name="cone">
                        <diffuse r=".54" g=".27" b=".07"/>
                    </object>
                </transblock>
            </object>
        </transblock>
        <!-- face -->
        <transblock>
            <translate x="0" y="0" z="-.05"/>
            <rotate x="1" y="0" z="0" angle="10"/>
            <scale x="1.05" y="1.05" z="1.05"/>
            <object type="tree" name="face">
                <!-- hair -->
                <transblock>
                    <translate x="0" y=".94" z=".93"/>
                    <rotate x="1" y="0" z="0" angle="120"/>
                    <scale x=".11" y=".28" z=".07"/>
                    <object type="primitive" name="cone">
                        <diffuse r="1" g="1" b="1"/>
                    </object>
                </transblock>
                <!-- eyes -->
                <transblock>
                    <translate x="0.08" y=".87" z=".93"/>
                    <scale x=".05" y=".05" z=".05"/>
                    <object type="primitive" name="sphere">
                        <diffuse r="0" g="0" b="0"/>
                    </object>
                </transblock>
                <transblock>
                    <translate x="-0.08" y=".87" z=".93"/>
                    <scale x=".05" y=".05" z=".05"/>
                    <object type="primitive" name="sphere">
                        <diffuse r="0" g="0" b="0"/>
                    </object>
                </transblock>
                <!-- top face -->
                <transblock>
                    <translate x="0" y=".78" z=".85"/>
                    <scale x=".25" y=".4" z=".3"/>
                    <object type="primitive" name="sphere">
                        <diffuse r=".54" g=".27" b=".07"/>
                    </object>
                </transblock>
                <!-- ears -->
                <transblock>
                    <translate x=".06" y="1.02" z=".85"/>
                    <scale x=".05" y=".12" z=".07"/>
                    <object type="primitive" name="cone">
                        <diffuse r=".54" g=".27" b=".07"/>
                    </object>
                </transblock>
                <transblock>
                    <translate x="-.06" y="1.02" z=".85"/>
                    <scale x=".05" y=".12" z=".07"/>
                    <object type="primitive" name="cone">
                        <diffuse r=".54" g=".27" b=".07"/>
                    </object>
                </transblock>
                <!-- base -->
                <transblock>
                    <translate x="0" y=".7" z="1.05"/>
                    <rotate x="1" y="0" z="0" angle="120"/>
                    <scale x=".23" y=".4" z=".3"/>
                    <object type="primitive" name="cone">
                        <diffuse r=".54" g=".27" b=".07"/>
                    </object>
                </transblock>
                <!-- nose -->
                <transblock>
                    <translate x="0" y=".62" z="1.15"/>
                    <rotate x="1" y="0" z="0" angle="-44"/>
                    <scale x=".11" y=".21" z=".1"/>
                    <object type="primitive" name="sphere">
                        <diffuse r=".54" g=".27" b=".07"/>
                    </object>
                </transblock>
            </object>
        </transblock>
        <!-- tail -->
        <transblock>
            <translate x="0" y=".17" z="-.75"/>
            <object type="tree" name="tail">
                <transblock>
                    <rotate x="1" y="0" z="0" angle="-60"/>
                    <scale x=".1" y=".1" z=".4"/>
                    <object type="primitive" name="sphere">
                        <diffuse r="1" g="1" b="1"/>
                    </object>
                </transblock>
                <transblock>
                    <translate x="0" y="-.68" z="-.058"/>
                    <rotate x="1" y="0" z="0" angle="180"/>
                    <scale x=".1" y="1.1" z=".1"/>
                    <object type="primitive" name="cone">
                        <diffuse r="1" g="1" b="1"/>
                    </object>
                </transblock>
            </object>
        </transblock>
    </object>


    <object type="tree" name="root">
        <!-- grass -->
        <transblock>
            <translate x="0" y="-.5" z=".5"/>
            <!-- <rotate x="0" y="0" z="1" angle="60"/> -->
            <scale x="3" y=".1" z="3"/>
            <object type="primitive" name="cube">
                <diffuse r="0" g="1" b="0"/>
            </object>
        </transblock>
        <!-- house -->
        <transblock>
            <object type="tree" name="house">
                <transblock>
                    <!-- roof -->
                    <object type="tree" name="roof">
                        <transblock>
                            <translate x="0.3" y=".5" z="0"/>
                            <rotate x="0" y="0" z="1" angle="60"/>
                            <scale x=".1" y=".75" z="1.5"/>
                            <object type="primitive" name="cube">
                                <diffuse r="1" g="1" b="1"/>
                            </object>
                        </transblock>
                        <transblock>
                            <translate x="-0.3" y=".5" z="0"/>
                            <rotate x="0" y="0" z="1" angle="-60"/>
                            <scale x=".1" y=".75" z="1.5"/>
                            <object type="primitive" name="cube">
                                <diffuse r="1" g="1" b="1"/>
                            </object>
                        </transblock>
                    </object>
                </transblock>
                <transblock>
                    <object type="tree" name="base">
                        <transblock>
                            <scale x=".8" y="1" z="1.3"/>
                            <object type="primitive" name="cube">
                                <diffuse r="1" g="0" b="0"/>
                            </object>
                        </transblock>
                        <!-- door -->
                        <transblock>
                            <translate x="0" y="-.2" z=".65"/>
                            <scale x=".5" y=".6" z=".05"/>
                            <object type="primitive" name="cube">
                                <diffuse r="0" g="0" b="0"/>
                            </object>
                        </transblock>
                        <transblock>
                            <translate x="0" y="-.2" z=".67"/>
                            <scale x=".4" y=".5" z=".05"/>
                            <object type="primitive" name="cube">
                                <diffuse r="1" g="1" b="1"/>
                            </object>
                        </transblock>
                    </object>
                </transblock>
            </object>
        </transblock>
        <!-- fences -->
        <transblock>
            <object type="tree" name="fence">
                <!-- sides -->
                <transblock>
                    <object type="tree" name="sides">
                        <!-- side 1 -->
                        <transblock>
                            <translate x="1.3" y="-.1" z=".5"/>
                            <!-- <rotate x="0" y="0" z="1" angle="60"/> -->
                            <scale x=".05" y=".05" z="2.7"/>
                            <object type="primitive" name="cube">
                                <diffuse r="1" g="1" b="1"/>
                            </object>
                        </transblock>
                        <transblock>
                            <translate x="1.3" y="-.3" z=".5"/>
                            <!-- <rotate x="0" y="0" z="1" angle="60"/> -->
                            <scale x=".05" y=".05" z="2.7"/>
                            <object type="primitive" name="cube">
                                <diffuse r="1" g="1" b="1"/>
                            </object>
                        </transblock>
                        <!-- side 2 -->
                        <transblock>
                            <translate x="0" y="-.1" z="1.8"/>
                            <rotate x="0" y="1" z="0" angle="-90"/>
                            <scale x=".05" y=".05" z="2.7"/>
                            <object type="primitive" name="cube">
                                <diffuse r="1" g="1" b="1"/>
                            </object>
